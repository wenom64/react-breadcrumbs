{
  "name": "react-breadcrumbs",
  "version": "1.5.2",
  "description": "Automatic breadcrumbs for react-router",
  "main": "index.js",
  "scripts": {
    "test": "find test/*.js -print0 | xargs -0 ./node_modules/.bin/babel-tape-runner",
    "build:dev": "cd demo && ../node_modules/.bin/webpack && cd ..",
    "watch:dev": "sane './node_modules/.bin/babel-tape-runner test/dom.js' --glob=**/*.js*"
  },
  "npmName": "react-breadcrumbs",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/svenanders/react-breadcrumbs.git"
  },
  "keywords": [
    "react-component",
    "breadcrumbs",
    "react",
    "trail"
  ],
  "author": "Sven Anders Robbestad <anders@robbestad.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/svenanders/react-breadcrumbs/issues"
  },
  "homepage": "https://github.com/svenanders/react-breadcrumbs",
  "peerDependencies": {
    "react": ">=0.13.2 || ^15.2.1",
    "react-router": ">2.5.0 || ^3.0.0-beta.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.5.0",
    "babel-tape-runner": "^2.0.0",
    "eslint": "^3.8.1",
    "history": "^4.3.0",
    "minify": "^2.0.12",
    "postcss": "^5.2.5",
    "postcss-cli": "^2.6.0",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^2.0.0",
    "tape": "^4.2.2",
    "uglifyjs": "^2.4.10",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "exenv": "^1.2.0"
  }
}
